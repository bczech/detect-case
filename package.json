{
  "name": "detect-case",
  "description": "Detects the casing of the input string (camelcase, lowercase, snakecase etc).",
  "version": "1.0.0",
  "homepage": "https://github.com/jonschlinkert/detect-case",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/detect-case",
  "bugs": {
    "url": "https://github.com/jonschlinkert/detect-case/issues"
  },
  "files": [
    "index.js"
  ],
  "license": "MIT",
  "scripts": {
    "test": "mocha"
  },
  "devDependencies": {
    "gulp-format-md": "^2.0.0",
    "mocha": "^6.1.4"
  },
  "keywords": [
    "camel",
    "camelcase",
    "case-sensitive",
    "case",
    "detect",
    "exec",
    "lowercase",
    "match",
    "matches",
    "occurrence",
    "occurrences",
    "pattern",
    "patterns",
    "regex",
    "regular expression",
    "replace",
    "replacement",
    "snake",
    "snakecase",
    "string",
    "substring",
    "title",
    "titlecase",
    "uppercase"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "lint": {
      "reflinks": true
    }
  }
}